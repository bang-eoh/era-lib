{
  "name": "ERa",
  "version": "0.5.0",
  "description": "An IoT Market Enabler! It supports WiFi, Ethernet, Zigbee, Modbus, Serial. Works with boards like Arduino, ESP8266, ESP32, STM32, Raspberry Pi...",
  "keywords": "ERa, esp8266, esp32, http, mqtt, zigbee, modbus, sensors, control, device, smartphone, mobile, app, web, cloud, communication, protocol, iot, wifi, ethernet, serial",
  "authors":
  {
    "name":  "EoH Ltd",
    "email": "info@eoh.io",
    "url":   "https://github.com/eoh-jsc",
    "maintainer": true
  },
  "repository":
  {
    "type": "git",
    "url": "https://github.com/eoh-jsc/era-lib.git"
  },
  "homepage": "https://e-ra.io",
  "dependencies": [
    {
      "owner": "vshymanskyy",
      "name": "Preferences",
      "version": "*",
      "platforms": ["espressif8266"],
      "frameworks": ["arduino"]
    },
    {
      "owner": "stm32duino",
      "name": "STM32duino FreeRTOS",
      "version": "*",
      "platforms": ["ststm32"],
      "frameworks": ["arduino"]
    },
    {
      "owner": "vshymanskyy",
      "name": "TinyGSM",
      "version": "*",
      "platforms": ["ststm32"],
      "frameworks": ["arduino"]
    }
  ],
  "export": {
    "exclude": [
      "linux",
      "extras",
      "tests"
    ]
  },
  "build": {
    "extraScript": "scripts/after_install.py",
    "libCompatMode": "strict"
  },
  "frameworks": "*",
  "platforms": "*",
  "examples": "examples/*/*.ino"
}
